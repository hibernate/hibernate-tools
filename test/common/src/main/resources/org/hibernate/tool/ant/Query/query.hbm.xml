<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hibernate.tool.ant.Query">
<!-- 
  This mapping demonstrates basic attributes of the mapping document

     (1) Simple id and properties        
     (2) Id generation with no arguments.
-->
    <class name="SerializableResult" table="SERIALIZABLE_RESULT">
        <id name="id" access="field">
        		<generator class="assigned"/>    		
    		</id>   	
    		<property name="length" type="int" access="field"/>   		  
    </class>

    <class name="ObjectResult" table="OBJECT_RESULT">
        <id name="id" access="field">
        		<generator class="assigned"/>    		
    		</id>   	
    		<property name="length" type="int" access="field"/>   		  
    </class>

</hibernate-mapping>
