<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "https://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class catalog="BARDB" name="Foo" optimistic-lock="none" schema="PUBLIC" table="FOO">
        <id name="id" type="int">
            <column name="ID"/>
            <generator class="assigned"/>
        </id>
        <property name="baz" type="string">
            <column length="256" name="BAZ"/>
        </property>
    </class>
</hibernate-mapping>
